<!DOCTYPE html>
<html {{{html-html-attributes}}}>
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	{{{html-headelement}}}
</head>
<body {{{html-body-attributes}}}>
	<div class="forest-wrapper">
		<header class="forest-header">
			<div class="forest-header-container">
				<div class="forest-logo">
					{{{data-portlets-sidebar.data-portlets.p-logo}}}
				</div>
				<nav class="forest-navigation">
					{{#data-portlets-sidebar.data-portlets}}
					{{^p-logo}}
					<div class="forest-nav-section">
						<h3>{{html-heading}}</h3>
						<ul>
							{{#array-items}}
							<li {{html-attributes}}>
								{{{html-link}}}
							</li>
							{{/array-items}}
						</ul>
					</div>
					{{/p-logo}}
					{{/data-portlets-sidebar.data-portlets}}
				</nav>
				<div class="forest-user-tools">
					{{#data-personal-menu}}
					<div class="forest-personal-menu">
						<ul>
							{{#array-items}}
							<li {{html-attributes}}>
								{{{html-link}}}
							</li>
							{{/array-items}}
						</ul>
					</div>
					{{/data-personal-menu}}
				</div>
			</div>
		</header>

		<main class="forest-main">
			<div class="forest-content-container">
				<div class="forest-page-tools">
					{{#data-page-tools}}
					<nav class="forest-page-tools-nav">
						<h3>{{msg-page-tools-label}}</h3>
						<ul>
							{{#array-items}}
							<li {{html-attributes}}>
								{{{html-link}}}
							</li>
							{{/array-items}}
						</ul>
					</nav>
					{{/data-page-tools}}
				</div>

				<article class="forest-content">
					{{#html-site-notice}}
					<div class="forest-site-notice">
						{{{html-site-notice}}}
					</div>
					{{/html-site-notice}}

					{{#html-user-language-attributes}}
					<div class="forest-first-heading" {{{html-user-language-attributes}}}>
						{{{html-title}}}
					</div>
					{{/html-user-language-attributes}}

					{{#html-subtitle}}
					<div class="forest-subtitle">
						{{{html-subtitle}}}
					</div>
					{{/html-subtitle}}

					{{#html-undelete-link}}
					<div class="forest-undelete">
						{{{html-undelete-link}}}
					</div>
					{{/html-undelete-link}}

					<div class="forest-body" {{{html-user-language-attributes}}}>
						{{{html-body-content}}}
					</div>

					{{#html-categories}}
					<div class="forest-categories">
						{{{html-categories}}}
					</div>
					{{/html-categories}}

					<div class="forest-footer-content">
						{{{html-after-content}}}
					</div>
				</article>
			</div>
		</main>

		<footer class="forest-footer">
			<div class="forest-footer-container">
				{{#data-footer}}
				<ul class="forest-footer-list">
					{{#array-items}}
					<li {{html-attributes}}>
						{{{html}}}
					</li>
					{{/array-items}}
				</ul>
				{{/data-footer}}
			</div>
		</footer>
	</div>
	{{{html-printtail}}}
</body>
</html>
